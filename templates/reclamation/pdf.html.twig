<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">

		{% block title %}
			<title>Details De Reclamation</title>
		{% endblock %}

		<!-- Bootstrap icons CSS -->
		{% block css %}
			<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.6.0/font/bootstrap-icons.css">
			<link rel="stylesheet" type="text/css" href="http://localhost/oneway/templates/reclamation/pdf.css">
		{% endblock %}

	</head>

	{% block body %}
		<body>
			<div class="container">
				
				{% for reclamation in reclamation %}
					<h2>Merci d'avoir diposer votre reclamation cher :
						{{ reclamation.id_user.name }}
						{{ reclamation.id_user.lastname }}
					</h2>
					<table class="info">
						<caption>Détails de votre reclamation :</caption>
						<thead>
							<tr>
							
								<th>Type de reclamation</th>
								<th>Texte de reclamation</th>
								<th> Texte de reponse</th>
								
							</tr>
						</thead>
						<tbody>

							<tr>
							<td>{{ reclamation.getIdtype.type }}</td>
								<td>{{ reclamation.text_rec }}
								</td>
								
								
								<td>{{ reclamation.reponse.text_rep }}</td>
							</tr>
						{% endfor %}

					</tbody>

				</table>

				<img src="data:image/png;base64," class="car">
				<h5>
				Nous aprécions votre patience et votre fidélit
				</h5>
				<div class="rights-reserved">

					All rights reserved &copy; 2023
					<h4>
						-Fancyart-
					</h4>

				</div>
			</div>

		</body>

	{% endblock %}
</html>
